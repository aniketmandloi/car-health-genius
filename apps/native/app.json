{
  "expo": {
    "scheme": "car-health-genius",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "bundler": "metro"
    },
    "name": "car-health-genius",
    "slug": "car-health-genius",
    "plugins": [
      "expo-font",
      [
        "react-native-ble-plx",
        {
          "isBackgroundEnabled": false,
          "neverForLocation": false,
          "bluetoothAlwaysPermission": "Allow Car Health Genius to connect to OBD-II adapters."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
