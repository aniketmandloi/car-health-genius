{
  "name": "car-health-genius",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test && node --test tests/**/*.test.mjs",
    "test:ci": "turbo test && node --test tests/**/*.test.mjs",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F server dev",
    "db:push": "turbo -F @car-health-genius/db db:push",
    "db:studio": "turbo -F @car-health-genius/db db:studio",
    "db:generate": "turbo -F @car-health-genius/db db:generate",
    "db:migrate": "turbo -F @car-health-genius/db db:migrate"
  },
  "dependencies": {
    "@car-health-genius/env": "workspace:*",
    "dotenv": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@car-health-genius/config": "workspace:*",
    "@types/node": "^22.13.14",
    "turbo": "^2.6.3",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.15.1"
}
